  <script type="text/javascript">
    (function() {

      var navbar = document.getElementById("nav");
      var sticky = navbar.offsetTop;

      function stickyNav() {
        if ($(".site-container").offset().top <= sticky) {
          navbar.classList.add("sticky")
          {#  $(".nav-img-ct img").attr("src", "/assets/gems-log-purple.png");  }
        } 
        
         if ($(".site-container").offset().top == 0 
         ||  $(".site-container").offset().top >= sticky) {
          navbar.classList.remove("sticky");
          $(".nav-img-ct img").attr("src", "/assets/gems-logo.png");
        }
      }

      function scrollS() {
            $(".swipe-cont h1").addClass("fadeUpDim");
            var tiemout = setTimeout(() => {
                $(".swipe-cont .swiper").addClass("fadeUp");
                $(".swipe-cont h1 span").addClass("colorChange");
            }, 3000);
        }

        function isElementInViewport(elem) {
            var $elem = $(elem);

            var scrollElem = ((navigator.userAgent.toLowerCase().indexOf('webkit') != -1) ? 'body' : 'html');
            var viewportTop = $(scrollElem).scrollTop();
            var viewportBottom = viewportTop + $(window).height();

            var elemTop = Math.round( $elem.offset().top );
            var elemBottom = elemTop + $elem.height();

            return ((elemTop < viewportBottom) && (elemBottom > viewportTop));
        }

         function animateKeyPoints() {
              var $elem = $('.what-we-do .key-points  .key-point');
              var $animateEl = $('.what-we-do .key-points  .key-point:nth-child(1)');
              var $animateEl2 = $('.what-we-do .key-points .key-point:nth-child(3)');
              
              if (isElementInViewport($elem)) {
                  $animateEl.addClass('moveIn_key1');
                  $animateEl2.addClass('moveIn_key2');
              } else {
                  {#  $elem.removeClass('start');  }
              }
        }


      $(document).ready(() => {
           const progressCircle = document.querySelector(".autoplay-progress svg");
    const progressContent = document.querySelector(".autoplay-progress span");
   
            var swiper = new Swiper('.swiper', {
              speed: 1000,
               slidesPerView: 3,
                spaceBetween: 30,
                grabCursor: true,
                stopOnLastSlide: true,
                autoplay: {
                  delay: 3000,
                  disableOnInteraction: false,
                },
                on: {
                  autoplayTimeLeft(s, time, progress) {
                    progressCircle.style.setProperty("--progress", 1 - progress);
                  }
                }
              });

            let alreadAnimated = false;

            $("body").on("scroll", function() {
                stickyNav();
                animateKeyPoints();

                    $(".facts .key-points .key-point").each((i, el)  => {
                      var top_of_element = $(el).offset().top;
                      var bottom_of_element = $(el).offset().top + $(this).outerHeight();
                      var bottom_of_screen = $(window).scrollTop() + window.innerHeight;
                      var top_of_screen = $(window).scrollTop();

                      if((bottom_of_screen > top_of_element) && (top_of_screen < bottom_of_element)){
                         {#  $(el).addClass("background")  }
                      } else {
                          {#  $('.custom-warning').hide();  }
                      }
                  })
           });

          {#  document.querySelector('video').playbackRate = .5;  }


            $(".board-ct .img-ct img").each((index, el) => {
                  $(el).on("click", () => {
                      let link = $(el).data("profile");
                   
                      $(".text-stack").fadeOut();
                    $(".board-ct").removeClass("moveUp");
                      $(".board-ct").addClass("moveDown");
                      

                      if ($(".members-ct").find(`[data-profile-link=` + link + `]`).hasClass("hide")) {
                          $(".board-ct .exit-ct").fadeIn();
                          $(".board-ct .exit-ct").addClass("show");

                           $(".members-ct").addClass("fadeUp");
                           $(".members-ct").removeClass("fadeDown");
                           
                          $(".members-ct .member").addClass("hide");
                          $(".members-ct").find(`[data-profile-link=` + link + `]`).removeClass("hide");
                          $(".members-ct").find(`[data-profile-link=` + link + `]`).addClass("show");
                      } 

                      {#  $(".members-ct").find(`[data-profile-link='2']`).removeClass("hide");
                      $(".members-ct").find(`[data-profile-link='2']`).addClass("show");  }
                  });

              })

              $(".board-ct .exit-ct").on("click", () => {
                    $(".text-stack").fadeIn();

                    $(".board-ct .exit-ct").fadeOut();
                    $(".members-ct").addClass("fadeDown");
                    $(".members-ct").removeClass("fadeUp");


                    $(".board-ct").removeClass("moveDown");
                    $(".board-ct").addClass("moveUp");
              })

                var legacyData = [ 
                {"id": 1, "name": "Sarah", "subtitle" : "MSU Graduate | Business Owner ", "testimony": "REAL GEMS was a divine blessing, offering a profound framework of servant leadership, love, and self-assurance that I hadn't realized I lacked. My experience with GEMS proved to be a resource for healing, a haven for security, and a close-knit community fostering sisterhood.", "imgSrc":"assets\/gems-legacy-1.png", "supportLink":"www.google.com"}, 
                {"id": 1, "name": "Jordyn", "subtitle" : "", "testimony": "Being A GEM has been a transformative experience for me. Having a person like Ms. Hill in my life at a young age set the foundation for me to move beyond what I thought I knew. Ms. Hill has always had an eye for seeing the hidden potential and pulling that out of you, whether you see it yourself or not. Being a member of REAL G.E.M.S challenges your perspectives, encourages personal growth, and creates a sisterhood of empowering women. Ms. Hill's genuine excitement and encouragement for the young generation of women to grow into the best versions of themselves is so pure, which is why REAL G.E.M.S will continue to flourish.", "imgSrc":"assets\/gems-legacy-2.png", "supportLink":"www.google.com"}, 
                {"id": 1, "name": "Teka", "subtitle" : "", "testimony": "REAL GEMS was something I never thought I would be able to fit into due to my confidence level at the time, until the ladies already involved showed me it's a confidence booster! Having a community of supportive women and an even more supportive mentor gave me the head start I didn't know I needed.", "imgSrc":"assets\/gems-legacy-3.png", "supportLink":"www.google.com"},
                {"id": 1, "name": "Kaylyn", "subtitle" : "", "testimony": "REAL GEMS helped me get out of my shell and made me feel good about being a part of something other than basketball. I'll always remember the foundation it gave me as a young woman and the real-life skills it taught me. I'm currently a remote Medical Coder and am proud to say that GEMS will always play a part as I excel in life and my career.", "imgSrc":"assets\/gems-legacy-4.png", "supportLink":"www.google.com"},
                {"id": 1, "name": "Allie", "subtitle" : "", "testimony": "REAL G.E.M.S. has helped me foster lifelong friendships and empowered me to be a part of organizations surrounded by other successful women. A truly unforgettable mentorship experience!", "imgSrc":"assets\/gems-legacy-5.png", "supportLink":"www.google.com"},
                {"id": 1, "name": "Colbi", "subtitle" : "", "testimony": "REAL GEMS helped lay the foundation for my walk into womanhood. It helped me to improve my self-esteem and self-worth and has helped make me into the woman I am today.", "imgSrc":"assets\/gems-legacy-6.png", "supportLink":"www.google.com"},
                {"id": 1, "name": "Jayla", "subtitle" : "", "testimony": "REAL GEMS has played such a pivotal role in who I am today and the woman I am growing into. To have learned how to love myself, treat others with kindness, and value sisterhood at such a crucial time in my adolescence, has truly empowered me to maintain success!", "imgSrc":"assets\/gems-legacy-7.png", "supportLink":"www.google.com"},
                {"id": 1, "name": "Trinity", "subtitle" : "Dental Assistant", "testimony": "I truly believe that without REAL GEMS, I wouldn't be the woman I am today. The invaluable lessons I acquired not only motivated me to evolve into an inspirational leader but also shaped me into the strong and resilient woman I am today. GEMS instilled in me the significance of giving back to my community, the true essence of sisterhood, and the importance of maintaining high standards in all aspects of my life. Thanks to the exceptional mentorship provided by Ms. Hill, I genuinely wouldn't be the amazing woman I am today without her love and this life-changing program.", "imgSrc":"assets\/gems-legacy-8.png", "supportLink":"www.google.com"},
                {"id": 1, "name": "Amber", "subtitle" : "Dental Assistant", "testimony": "I love the Gems", "imgSrc":"assets\/gems-legacy-10.png", "supportLink":"https://www.google.com"},
                {"id": 1, "name": "Sab", "subtitle" : "Dental Assistant", "testimony": "I love the Gems", "imgSrc":"assets\/gems-legacy-11.png", "supportLink":"https://www.google.com"},
                {"id": 1, "name": "Sab", "subtitle" : "Dental Assistant", "testimony": "I love the Gems", "imgSrc":"assets\/gems-legacy-12.png", "supportLink":"https://www.google.com"},
                {"id": 1, "name": "Sab", "subtitle" : "Dental Assistant", "testimony": "I love the Gems", "imgSrc":"assets\/gems-legacy-13.png", "supportLink":"https://www.google.com"}
                ];

              $.each(legacyData, (i, legacy) => {
                  $(".legacy-ct").append("<div class=\"legacy mb-5 bg-white p-2 rounded-lg grid grid-cols-3 grid-rows-1 gap-2\">\<div style=\"max-height: 250px;\" class=\"w-4\/5 m-auto col-start-1 col-span-1 bg-black rounded-lg overflow-hidden\"><img class=\"w-full h-auto \" src=\"" + legacy.imgSrc + "\" alt=\"\"> <\/div> <div class=\"info p-2 rounded-lg  col-start-2 col-span-2 \"><div class=\" grid grid-cols-5 gap-4  p-2 info-header \"><div class=\"col-span-2 badge-ct  m-auto ms-0 mx-2 rounded-lg bg-blue-600 p-2\"><h2 class=\"text-sm text-center text-white font-bold\">2004 CLASS<\/h2><\/div><div class=\"col-span-3  info-personal flex flex-col items-start\">\r\n                                      <h5 class=\"text-md font-bold\">" + legacy.name + "<\/h5>\r\n                                      <p class=\"text-xs font-bold text-gray-600\">" + legacy.subtitle + "<\/p>\r\n                                  <\/div>\r\n                                <\/div>\r\n                                <p class=\"my-3 text-sm text-gray-800 text-left\">"  + legacy.testimony + "<\/p>\r\n\r\n                                 <a href=\"" + legacy.supportLink + "\" class=\"cursor-pointer border border-gray-500 hover:border-blue-500 rounded-lg flex flex-row items-center justify-evenly\">\r\n                                    <div class=\"gap-4 rounded-lg p-2 grid grid-cols-6 flex flex-row items-center\">\r\n                                        <div class=\"w-10 h-10\">\r\n                                          <img class=\"w-full m-auto\" src=\"assets\/gift-icon.png\">\r\n                                        <\/div>\r\n                                        <div class=\"col-span-4 flex flex-col items-start justify-start\">\r\n                                          <p class=\"text-sm font-bold text-left text-black\">Support " + legacy.name + "'s Business<\/p>\r\n                                          <p class=\"text-xs text-left text-gray-500\">I'm raising money for my new business<\/p>\r\n                                        <\/div>\r\n                                         <div class=\"col-span-1 \">\r\n                                          <img class=\"w-full m-auto\" src=\"assets\/Paypal-logo.png\">\r\n                                        <\/div>\r\n                                    <\/div>\r\n                                 <\/a>\r\n\r\n                            <\/div>\r\n                        <\/div>");
              });

      })
    })();
  </script>

  <script type="text/javascript">
    $(function() {

      var navbar = document.getElementById("nav");
      var sticky = navbar.offsetTop;

      function stickyNav() {
        if ($(".site-container").offset().top <= sticky) {
          navbar.classList.add("sticky")
          {#  $(".nav-img-ct img").attr("src", "/assets/gems-log-purple.png");  }
        } 
        
         if ($(".site-container").offset().top == 0 
         ||  $(".site-container").offset().top >= sticky) {
          navbar.classList.remove("sticky");
          $(".nav-img-ct img").attr("src", "/assets/gems-logo.png");

        }
      }

      $("body").on("scroll", function() {
        {#  alert("he")  }
            stickyNav();
          });

        var myInterval = ""; 
          var width = 1;

        function myTimer() {
            var elem = document.getElementById("progressBar");
              if (width >= 100) {
                clearInterval(myInterval);
                {#  document.location.href = "/free";  }
                i = 0;
              } else {
                width++;
                elem.style.width = width + "%";
              }
        }

        function myStopFunction() {
          width = 0;
          {#  alert(myInterval);  }
          clearInterval(myInterval);
        }
       

      $(document).ready(() => {
          
        $(".explore-trigger-ct").on("mouseenter", function() {
          $(".explore-trigger").html("Continue hovering...")
          myInterval = setInterval(myTimer, 50);
        })
         $(".explore-trigger-ct").on("mouseleave", function(){
            $(".explore-trigger").html(" Hover to explore"  )
            i = 0;
             $("#progressBar").css({width: "0px"});
             myStopFunction();
        })
      })
    })();
  </script>

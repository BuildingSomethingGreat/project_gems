        {#  <script src="https://cdn.jsdelivr.net/npm/swiper@9/swiper-bundle.min.js"></script>  }
  
  </head>
  <body class="gallery-body" id="{squarespace.page-id}" class="{squarespace.page-classes}">

        {@|apply nav.block}


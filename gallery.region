          {@|apply footer.block}

        </div>
  </body>

  <script type="text/javascript">
    (function() {

      var navbar = document.getElementById("nav");
      var sticky = navbar.offsetTop;

      function stickyNav() {
        if ($(".site-container").offset().top <= sticky) {
          navbar.classList.add("sticky")
          {#  $(".nav-img-ct img").attr("src", "/assets/gems-log-purple.png");  }
        } 
        
         if ($(".site-container").offset().top == 0 
         ||  $(".site-container").offset().top >= sticky) {
          navbar.classList.remove("sticky");
          $(".nav-img-ct img").attr("src", "/assets/gems-logo.png");
        }
      }

      function scrollCueSwipper() {
            $(".swipe-cont h1 span").addClass("colorChange");
            $(".swipe-section .swiper").addClass("fadeUp");
        }

      $(document).ready(() => {
            var swiper = new Swiper('.Slider-container', {
                effect: 'cards',
                grabCursor: true,
                centerdSlides: true,
                loop: true,
                on: {
                  slideChange: function() {
                    var active = this.realIndex; 
                    $(".swipe-section h1").fadeOut();
                    $(".swipe-section h1").removeClass("fadeInSoon");
                    $(".swipe-section h1[data-index='" + active +"']").fadeIn();
                    $(".swipe-section h1[data-index='" + active +"']").addClass("fadeInSoon");

                  }
                }
              });


            let alreadAnimated = false;
            $("body").on("scroll", function() {
                stickyNav();

                if (!alreadAnimated)  {
                    scrollCueSwipper();
                    alreadAnimated = true;
                }


          });
      })
    })();
  </script>
</html>


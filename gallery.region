                    OUT
                    <span >BRIGHTLY</span>
                    </h1>
                <div class="swiper Slider-container">
                    <div class="swiper-wrapper">
                       <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-1.jpeg" alt=""/>
                        </div>
                        
                          <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-2.jpeg" alt=""/>
                        </div>

                         <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-3.jpeg" alt=""/>
                        </div>
                        
                          <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-4.jpeg" alt=""/>
                        </div>

                         <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-5.jpeg" alt=""/>
                        </div>
                        
                          <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-6.jpeg" alt=""/>
                        </div>

                         <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-7.jpeg" alt=""/>
                        </div>
                        
                          <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-8.jpeg" alt=""/>
                        </div>

                         <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-9.jpeg" alt=""/>
                        </div>
                        
                          <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-10.jpeg" alt=""/>
                        </div>

                         <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-11.jpeg" alt=""/>
                        </div>
                        
                          <div class="swiper-slide">
                            <div class="overlay"></div>
                            <img src="assets/student-12.jpeg" alt=""/>
                        </div>

                    </div>
                    <p> Swipe to explore accomplishments and press.</p>
                </div>
            </div>
          {@|apply footer.block}

        </div>
  </body>

  <script type="text/javascript">
    (function() {

      var navbar = document.getElementById("nav");
      var sticky = navbar.offsetTop;

      function stickyNav() {
        if ($(".site-container").offset().top <= sticky) {
          navbar.classList.add("sticky")
          {#  $(".nav-img-ct img").attr("src", "/assets/gems-log-purple.png");  }
        } 
        
         if ($(".site-container").offset().top == 0 
         ||  $(".site-container").offset().top >= sticky) {
          navbar.classList.remove("sticky");
          $(".nav-img-ct img").attr("src", "/assets/gems-logo.png");
        }
      }

      function scrollCueSwipper() {
            $(".swipe-cont h1").addClass("fadeUpDim");
            var tiemout = setTimeout(() => {
                $(".swipe-cont .swiper").addClass("fadeUp");
                $(".swipe-cont h1 span").addClass("colorChange");
            }, 3000);
        }

      $(document).ready(() => {
            var swiper = new Swiper('.Slider-container', {
                effect: 'cards',
                grabCursor: true,
                centerdSlides: true,
                loop: true,
              });

            let alreadAnimated = false;
            $("body").on("scroll", function() {
                stickyNav();

                if (!alreadAnimated)  {
                    scrollCueSwipper();
                    alreadAnimated = true;
                }


          });
      })
    })();
  </script>
</html>


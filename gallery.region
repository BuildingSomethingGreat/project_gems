  <script type="text/javascript">
    $(function() {

      var navbar = document.getElementById("nav");
      var sticky = navbar.offsetTop;

      function stickyNav() {
        if ($(".site-container").offset().top <= sticky) {
          navbar.classList.add("sticky")
          {#  $(".nav-img-ct img").attr("src", "/assets/gems-log-purple.png");  }
        } 
        
         if ($(".site-container").offset().top == 0 
         ||  $(".site-container").offset().top >= sticky) {
          navbar.classList.remove("sticky");
          $(".nav-img-ct img").attr("src", "/assets/gems-logo.png");

        }
      }

      function triggerPillarAnimation() {
          if ($("body").offset().top > $(".gems-about-section").offset().top && !alreadAnimated) {
              $(".back-pillar")
                .animate({
                  left: $(".pillar:eq(0)").position().left + 42
                }, 1000)
                .delay(2000)
                .animate({
                  left: $(".pillar:eq(1)").position().left + 42
                }, 1000)
                .delay(2000)
                .animate({
                  left: $(".pillar:eq(2)").position().left + 42
                }, 1000)
                .delay(2000)
                .animate({
                  left: "100%"
                }, 1000);

              var delay = 0;
              for (var i = 0; i < 3; i++) {
                  $(".pillar:eq(" + i +") > p")
                    .delay(delay)
                    .animate({color: "black"}, 1000)
                    .delay(2000)
                    .animate({color: "white"}, 1000)
                    delay += 3000;
              }
              alreadAnimated = true;
          }
      }
      let alreadAnimated = false;
      $("body").on("scroll", function() {
        {#  alert("he")  }
            stickyNav();
            triggerPillarAnimation();
          });

        var myInterval = ""; 
          var width = 1;

        function myTimer() {
            var elem = document.getElementById("progressBar");
              if (width >= 100) {
                clearInterval(myInterval);
                {#  document.location.href = "/free";  }
                i = 0;
              } else {
                width++;
                elem.style.width = width + "%";
              }
        }

        function myStopFunction() {
          width = 0;
          {#  alert(myInterval);  }
          clearInterval(myInterval);
        }
       

      $(document).ready(() => {

        var base = $(".grad-card:nth(1)").position().top;
        var leftBase = $(".grad-card:nth(1)").offset().left;
        {#  alert(leftBase);  }
        var difference; 

        {#  if (winderinnerWidth)  }
        if (window.innerWidth <= 1440) {
            difference = 3;
        } else {
            difference = 2;
        }

       
       

        $(".interesting-text-ct").css({top: base, left: leftBase / difference});
        $(".interesting-text-ct").addClass("fadeIn");
        
        $(window).on("resize", function(){
            var base = $(".grad-card:nth(1)").position().top;
            var leftBase = $(".grad-card:nth(1)").offset().left;
            $(".interesting-text-ct").css({top: base, left: leftBase / difference});
        })

        $(".explore-trigger-ct").on("mouseenter", function() {
          $(".explore-trigger").html("Continue hovering...")
          myInterval = setInterval(myTimer, 50);
        })
         $(".explore-trigger-ct").on("mouseleave", function(){
            $(".explore-trigger").html(" Hover to explore"  )
            i = 0;
             $("#progressBar").css({width: "0px"});
             myStopFunction();
        })

        document.querySelectorAll('a[href^="#"]').forEach(anchor => {
                anchor.addEventListener('click', function (e) {
                    e.preventDefault();

                    document.querySelector(this.getAttribute('href')).scrollIntoView({
                        behavior: 'smooth'
                    });
                });
        });
       
      })
    })();
  </script>

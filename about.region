  <body class="about-body" id="{squarespace.page-id}" class="{squarespace.page-classes}">
        {@|apply nav.block}
      <div class="wrapper">
      
      <div class="site-container">
            <div class="splash-ct">
                <div class="text-stack">
                  <h3 class="fadeIn">
                    ABOUT
                  </h3>
                  <h1 class="fadeIn">
                        GET TO KNOW<br>
                        THE GEMS <br>
                      {#  <span class="colorChange_about">TOGETHER.</span>  }
                  </h1>
                  <p class="fadeIn">
                      <b >
                      Learn about the R.E.A.L GEMS organization and how we plan
